@article{ WOS:A1992JP20200005,
Author = {BEREJIKIAN, J},
Title = {REVOLUTIONARY COLLECTIVE ACTION AND THE AGENT-STRUCTURE PROBLEM},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1992},
Volume = {86},
Number = {3},
Pages = {647-657},
Month = {SEP},
Abstract = {Unraveling the nexus between agents and structures is fundamental to an
   understanding of political and social change. The two most prominent
   methodological approaches to explain revolutionary collective action
   involve either individual reductionism or structural reductionism. Both
   approaches result in theoretical inconsistencies and/or explanatory
   anomalies. An alternative proposed here utilizes the concept of framing
   developed in behavioral decision theory primarily by Quatrone and
   Tversky. It directly addresses the agent-structure problem by developing
   the proposition that individuals evoke alternative decision rules in
   different structural contexts. The result is greater theoretical
   coherence and resolution of anomalous cases. Additionally, this model
   begins to define a new role for ideology in explanations of
   revolutionary collective action.},
