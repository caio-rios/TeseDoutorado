@article{ WOS:A1993KP42800011,
Author = {BANASZAK, LA and PLUTZER, E},
Title = {CONTEXTUAL DETERMINANTS OF FEMINIST ATTITUDES - NATIONAL AND SUBNATIONAL
   INFLUENCES IN WESTERN-EUROPE},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1993},
Volume = {87},
Number = {1},
Pages = {147-157},
Month = {MAR},
Abstract = {We augment the survey-based studies of attitudes toward feminism with
   comparative, contextual perspectives emphasizing the importance of
   social structure and culture. In doing so, we are able to assess the
   relative merits of two very different structural theories. Most
   researchers implicitly assume a simple linear effect of social context
   on attitudes. On the other hand, some early works on American
   conservatism suggest to us that status discontent may be a better
   explanation. We explicate these two approaches and derive a series of
   testable hypotheses for each. We then examine the validity of these
   theories utilizing data from nine European nations.},
