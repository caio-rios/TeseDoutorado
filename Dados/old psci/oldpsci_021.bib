@article{ WOS:A1993LD49500007,
Author = {BURTT, S},
Title = {THE POLITICS OF VIRTUE TODAY - A CRITIQUE AND A PROPOSAL},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1993},
Volume = {87},
Number = {2},
Pages = {360-368},
Month = {JUN},
Abstract = {I argue for a distinction between publicly oriented and privately
   oriented conceptions of civic virtue.  I first provide a critique of two
   current politics of virtue (liberal and republican), arguing (1) that
   despite their differences, they are both publicly oriented and (2) that
   their problems lie in this public orientation. I conclude by arguing for
   the legitimacy and promise of a privately oriented politics of civic
   virtue, using as examples of this approach Cato's Letters from the early
   eighteenth, century and the work of two contemporary theorists, Bruce
   Ackerman and Stephen Elkin.},
