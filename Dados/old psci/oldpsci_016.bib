@article{ WOS:000072227100013,
Author = {Wood, BD and Peake, JS},
Title = {The dynamics of foreign policy agenda setting},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1998},
Volume = {92},
Number = {1},
Pages = {173-184},
Month = {MAR},
Abstract = {Theoretical and empirical work on public policy agenda setting has
   ignored foreign policy. We develop a theory of foreign policy agenda
   setting and test the implications using time-series vector
   autoregression and Box-Tiao (1975) impact assessment methods. We
   theorize an economy of attention to foreign policy issues driven by
   issue inertia, events external to U.S. domestic institutions, as well as
   systemic attention to particular issues. We also theorize that the
   economy of attention is affected by a law of scarcity and the rise and
   fall of events in competing issue areas. Using measures of presidential
   and media attention to the Soviet Union, Arab-Israeli conflict, and
   Bosnian conflict, we show that presidential and media attentions respond
   to issue inertia and exogenous events in both primary and competing
   issue areas. Media attention also affects presidential attention but the
   president does not affect issue attention by the media.},
