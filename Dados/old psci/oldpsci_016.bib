@article{ WOS:A1997XB67200006,
Author = {BoxSteffensmeier, JM and Arnold, LW and Zorn, CJW},
Title = {The strategic timing of position taking in congress: A study of the
   North American Free Trade Agreement},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1997},
Volume = {91},
Number = {2},
Pages = {324-338},
Month = {JUN},
Abstract = {A critical element of decision making is the timing of choices political
   actors make; often when a decision is mane is as critical as the
   decision itself. We posit a dynamic model of strategic position
   announcement based on signaling theories of legislative politics. We
   suggest that members who receive clear signals from constituents,
   interest groups, and policy leaders will announce their positions
   earlier. Those with conflicting signals will seek more information,
   delaying their announcement. We test several expectations by examining
   data on when members of the House of Representatives announced their
   positions on the North American Free Trade Agreement. We also contrast
   the timing model with a vote model, and find that there are meaningful
   differences between the factors influencing the timing of position
   announcements and vote choice. Our research allows analysts to interpret
