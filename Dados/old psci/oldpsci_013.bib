@article{ WOS:000088910600010,
Author = {Statmann, T},
Title = {Congressional voting over legislative careers: Shifting positions and
   changing constraints},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2000},
Volume = {94},
Number = {3},
Pages = {665-676},
Month = {SEP},
Abstract = {The proposed model predicts that voting behavior of legislators is more
   variable early in their career and that junior members are more likely
   to vote with their party than senior members. The results from the
   analysis of voting and variability of voting patterns in the House of
   Representatives and the Senate are consistent with the hypotheses: Party
   line voting and variability of voting decisions decline with increasing
   seniority. Changes in voting behavior are also induced by redistricting.
   The empirical results show that legislators subject to redistricting
   change their voting behavior to accord better with altered constituency
   preferences.},
