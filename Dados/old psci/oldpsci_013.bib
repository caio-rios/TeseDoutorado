@article{ WOS:000087697000006,
Author = {Clark, WR and Hallerberg, M},
Title = {Mobile capital, domestic institutions, and electorally induced monetary
   and fiscal policy},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2000},
Volume = {94},
Number = {2},
Pages = {323-346},
Month = {JUN},
Abstract = {The literature on global integration and national policy autonomy often
   ignores a central result from open economy macroeconomics: Capital
   mobility constrains monetary policy when the exchange rate is fixed and
   fiscal policy when the exchange rate is flexible. Similarly,
   examinations of the electoral determinants of monetary and fiscal policy
   typically ignore international pressures altogether. We develop a formal
   model to analyze the interaction between fiscal and monetary
   policymakers under various exchange rate regimes and the degrees of
   central bank independence. We test the model using data from OECD
   countries. We find evidence that preelectoral monetary expansions occur
   only when the exchange rate is flexible and central bank independence is
   low; preelectoral fiscal expansions occur when the exchange rate is
   fixed We then explore the implications of our model for arguments that
   emphasize the partisan sources of macroeconomic policy and for the
   conduct of fiscal policy after economic and monetary union in Europe.},
