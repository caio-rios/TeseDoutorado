@article{ WOS:000084142300007,
Author = {Hammons, CW},
Title = {Was James Madison wrong? Rethinking the American preference for short,
   framework-oriented constitutions},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1999},
Volume = {93},
Number = {4},
Pages = {837-849},
Month = {DEC},
Abstract = {American constitutional thought has long held that short,
   framework-oriented constitutions last longer than lengthy,
   statute-oriented constitutions. The longevity of the U.S. Constitution
   contributes heavily to this assumption. Not surprisingly, political
   scientists criticize state constitutions for their greater length and
   tendency to address issues better dealt with through ordinary statute
   law. These ``defects{''} are frequently cited as responsible for the
   shorter lifespan of state constitutions. An examination of the 145
   constitutions used by the American states since 1776, however, reveals a
   relationship among content, length, and durability that refutes the
   assumption that the design of the national constitution is necessarily
   superior. To the contrary, the analysis here reveals that longer and
   more detailed design of state constitutions actually enhances rather
   than reduces their longevity.},
