@article{ WOS:000792380300001,
Author = {Tai, Yuehong Cassandra and Hu, Yue and Solt, Frederick},
Title = {Democracy, Public Support, and Measurement Uncertainty},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {118},
Number = {1},
Pages = {512-518},
Month = {FEB},
Abstract = {Do democratic regimes depend on public support to avoid backsliding?
   Does public support, in turn, respond thermostatically to changes in
   democracy? Two prominent recent studies (Claassen 2020a; 2020b)
   reinvigorated the classic hypothesis on the positive relationship
   between public support for democracy and regime survival-and challenged
   its reciprocal counterpart-by using a latent variable approach to
   measure mass democratic support from cross-national survey data.
   However, both studies used only the point estimates of democratic
   support. We show that incorporating the concomitant measurement
   uncertainty into these analyses reveals that there is no support for
   either study's conclusion. Efforts to minimize the uncertainty by
   incorporating additional survey data still fail to yield evidence in
   support of either hypothesis. These results underscore the need for both
   more nuanced analyses of the relationships between public support and
   democracy and taking measurement uncertainty into account when working
   with latent variables.},
