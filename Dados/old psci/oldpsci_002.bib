@article{ WOS:000784284600007,
Author = {Atsusaka, Yuki},
Title = {A Logical Model for Predicting Minority Representation: Application to
   Redistricting and Voting Rights Cases},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
