@article{ WOS:000678562100025,
Author = {Masterson, Daniel and Yasenov, Vasil},
Title = {Does Halting Refugee Resettlement Reduce Crime? Evidence from the US
   Refugee Ban},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
