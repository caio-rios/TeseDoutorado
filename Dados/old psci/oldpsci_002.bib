@article{ WOS:000743369500001,
Author = {Hopkins, Daniel J. and Noel, Hans},
Title = {Trump and the Shifting Meaning of ``Conservative{''}: Using Activists'
   Pairwise Comparisons to Measure Politicians' Perceived Ideologies},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2022},
Volume = {116},
Number = {3},
Pages = {1133-1140},
Month = {AUG},
Abstract = {Although prior scholarship has made considerable progress in measuring
   politicians' positions, it has only rarely considered voters' or
   activists' perceptions of those positions. Here, we present a novel
   measure of U.S. senators' perceived ideologies derived from 9,030
   pairwise comparisons elicited from party activists in three 2016 YouGov
   surveys. By focusing on activists, we study a most-likely case for
   perceiving within-party ideological distinctions. We also gain empirical
   leverage from Donald Trump's nomination and heterodox positions on some
   issues. Our measure of perceived ideology is correlated with nominate
   but differs in informative ways: senators with very conservative voting
   records were sometimes perceived as less conservative if they did not
   support Trump. A confirmatory test shows these trends extended into
   2021. Even among activists, perceived ideology appears to be anchored by
   prominent people as well as policy positions.},
