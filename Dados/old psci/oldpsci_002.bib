@article{ WOS:000784284600017,
Author = {Muchlinski, David},
Title = {Swords and Plowshares: Property Rights, Collective Action, and Nonstate
   Governance in the Jewish Community of Palestine 1920-1948},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2021},
Volume = {115},
Number = {4},
Pages = {1373-1387},
Month = {NOV},
Abstract = {Developing states lacking a monopoly over the use of force are commonly
   seen as having failed to live up to the ideal Weberian sovereign type.
   Yet rather than being a calling card of anarchy, the devolution of
   important state functions to subnational actors is a rational strategy
   for developing states to effectively provide important public goods. The
   case study of the Jewish Community of Palestine demonstrates one
   instance where subnational communities provided public goods. This study
   highlights the causal effect of property rights within institutions to
   drive behavior consistent with the provision of public and private
   goods. Analyzing temporal and institutional variation across two
   agricultural communities demonstrates a unique strategy of subnational
   governance and public goods provision in a developing state. Devolution
   of public goods provision to subnational actors may be an alternative
   strategy of governance for developing states that are not yet able to
   effectively provide important public goods.},
