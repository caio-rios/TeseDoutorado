@article{ WOS:000892929400001,
Author = {Ritholtz, Samuel and Buxton, Rebecca},
Title = {Sanctuary after Asylum: Addressing a Gap in the Political Theory of
   Refuge},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2023},
Volume = {117},
Number = {3},
Pages = {1166-1171},
Month = {AUG},
Abstract = {This research note argues that political theorists of refuge ought to
   consider the experiences of refugees after they have received asylum in
   the Global North. Currently, much of the literature concerning the
   duties of states toward refugees implicitly adopts a blanket approach,
   rather than considering how varied identities may affect the remedies
   available to displaced people. Given the prevalence of racism,
   xenophobia, and homophobia in the Global North, and the growing norm of
   dissident persecution in foreign territory, protection is not guaranteed
   after either territorial or legal admission. This research note
   considers the case of LGBTQ refugees in order to demonstrate the
   analytical potential of more inclusive and diverse normative approaches.
   Taking the origin and extension of harm seriously requires a
   conceptualization of sanctuary after asylum that accurately reflects the
   experiences of the displaced. In doing so, questions arise regarding the
   nature and efficacy of territorial asylum.},
