@article{ WOS:001157406400001,
Author = {Hewitt, Luke and Broockman, David and Coppock, Alexander and Tappin, Ben
   m. and Slezak, James and Coffman, Valerie and Lubin, Nathaniel and
   Hamidian, Mohammad},
Title = {How Experiments Help Campaigns Persuade Voters: Evidence from a Large
   Archive of Campaigns' Own Experiments},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {118},
Number = {4},
Pages = {2021-2039},
Month = {NOV},
Abstract = {Political campaigns increasingly conduct experiments to learn how to
   persuade voters. Little research has considered the implications of this
   trend for elections or democracy. To probe these implications, we
   analyze a unique archive of 146 advertising experiments conducted by US
   campaigns in 2018 and 2020 using the platform Swayable. This archive
   includes 617 advertisements produced by 51 campaigns and tested with
   over 500,000 respondents. Importantly, we analyze the complete archive,
   avoiding publication bias. We find small but meaningful variation in the
   persuasive effects of advertisements. In addition, we find that common
   theories about what makes advertising persuasive have limited and
   context-dependent power to predict persuasiveness. These findings
   indicate that experiments can compound money's influence in elections:
