@article{ WOS:001111348500001,
Author = {Eksteen, Riaan},
Title = {Frederik W. de Klerk: Presidential Years in the International Arena},
Journal = {POLITICAL SCIENCE QUARTERLY},
Year = {2024},
Volume = {139},
Number = {1},
Pages = {59-77},
Month = {FEB 29},
Abstract = {Frederik W. de Klerk left numerous bookmarks in South Africa's history.
   During his presidency (1989-1994), many of those relate to his handling
   of foreign affairs. When he became president, he was a novice in this
   field. He made a quantum political leap on 2 February 1990 when he
   released Nelson Mandela from prison and unbanned the African National
   Congress. He engaged in personal diplomacy and forged remarkable
   relationships with President George H. W. Bush and Prime Minister
   Margaret Thatcher. His decision to destroy South Africa's nuclear
   arsenal led to the signing of the Non-Proliferation of Nuclear Weapons
   Treaty in July 1991. He evaluated the events in Eastern Europe, the fall
   of the Berlin Wall, and demise of the Soviet Union in their proper
   context. He abolished apartheid. He did not attempt to reform it as
   Gorbachev did with the Soviet system. He adjusted South Africa's foreign
   policy accordingly when the geo-political environment changed in
   Southern Africa with Cuban withdrawal from Angola and Namibia's
   independence in March 1990. In December 1993, he shared the Nobel Peace
   Prize with Mandela. Up to now FW's legacy in the realm of international
   relations have not been properly explored.},
