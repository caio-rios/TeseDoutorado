@article{ WOS:001282599600001,
Author = {Copus, Ryan and Huebert, Ryan and Pellaton, Paige},
Title = {Trading Diversity? Judicial Diversity and Case Outcomes in Federal
   Courts},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2024},
Month = {2024 AUG 2},
Abstract = {Are federal lawsuits resolved differently based on the race or gender of
   the judges assigned to hear them? Recent empirical research posits that
   women and judges of color decide cases more liberally, at least in some
   identity-salient areas of law. However, these studies analyze small
   numbers of cases and judges, and use research designs that limit their
   causal interpretations. Using an original dataset of all civil rights
   cases filed in 20 federal district courts over multiple decades and a
   strong causal identification strategy, we find that assignment of cases
   to judges of color or women has no statistically significant effect on
   case outcomes among Democratic appointees. However, it causes more
   conservative outcomes among Republican appointees. We explain these
   results with a theory of bargaining over judicial appointments in which
   Republican presidents take advantage of Democrats' preference for
   diversity on the bench to appoint more conservative judges.},
