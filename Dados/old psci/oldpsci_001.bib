@article{ WOS:001074739200001,
Author = {Cooperman, Alicia Dailey},
Title = {Bloc Voting for Electoral Accountability},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {118},
Number = {3},
Pages = {1222-1239},
Month = {AUG},
Abstract = {How do citizens hold local politicians accountable? I argue that
   citizens, especially through neighborhood associations, can use bloc
   voting as a bottom-up, grassroots strategy to pressure politicians for
   public services. Politicians monitor polling station voting, and
   communities switch allegiance if politicians do not deliver. I measure
   the perceived and actual relationships between community
   characteristics, bloc voting, and water access-an essential resource
   prone to political manipulation. I analyze an original household survey
   and conjoint experiment merged with electoral data in rural Brazil, and
   qualitative interviews illustrate theoretical mechanisms. Bloc voting is
   more likely in communities with high trust and participation, and bloc
   voting improves water access for association members. However, this
   strategy is only worthwhile for communities that can demonstrate their
   vote at their polling station. In contrast to top-down explanations of
   bloc voting, I highlight the interaction of collective action and
   electoral institutions for accountability and public service provision.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {EDINBURGH BLDG, SHAFTESBURY RD, CB2 8RU CAMBRIDGE, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Cooperman, AD (Corresponding Author), George Washington Univ, Dept Polit Sci \& Int Affairs, Washington, DC 20052 USA.
   Cooperman, Alicia Dailey, George Washington Univ, Dept Polit Sci \& Int Affairs, Washington, DC 20052 USA.},
