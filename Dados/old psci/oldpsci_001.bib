@article{ WOS:001361448000001,
Author = {Curry, James M. and Roberts, Jason M.},
Title = {Interpersonal Relationships, Bipartisanship, and January 6th},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2024},
Month = {2024 NOV 21},
Abstract = {For a member of Congress to be a successful lawmaker, they must work
   collaboratively with their colleagues. Previous work has found that
   interpersonal relationships among lawmakers are a key predictor of
   legislative collaboration-particularly among members from opposing
   parties. In the wake of the events of January 6, 2021, many Democratic
   lawmakers claimed that their relationships with some of their Republican
   colleagues were irretrievably broken and they would be unable to
   continue collaborating with members who voted against certifying
   electoral votes from the 2020 presidential election. Using data on
   original cosponsorship and legislative effectiveness from the 115th to
   117th Congresses (2017-22) we find that Republican lawmakers who voted
   against the certification of electoral votes lost bipartisan
   collaborators and saw a drastic decrease in their legislative
   effectiveness in the 117th Congress.},
