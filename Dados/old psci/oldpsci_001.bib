@article{ WOS:001262743700001,
Author = {Goodwin, Jeff},
Title = {The New Urban Rebellions: A Review Article},
Journal = {POLITICAL SCIENCE QUARTERLY},
Year = {2024},
Month = {2024 JUN 25},
Abstract = {Mark R. Beissinger's The Revolutionary City is a tour de force that will
   interest a wide range of readers. It charts both the decline of social
   revolutions and the rise of a new kind of rebellion, what Beissinger
   calls the urban civic revolt. Beissinger also analyzes the many ways in
   which these revolts differ from previous ones. These new rebellions have
   been minimalist in character, aiming to overthrow despotic regimes but
   eschewing any effort to redistribute wealth on a large scale or
   transform class relations. Beissinger's ``spatial theory of
   revolution,{''} however, only partially illuminates these new urban
   rebellions, and it leads him to the premature conclusion that social
   revolutions will never occur again.},
