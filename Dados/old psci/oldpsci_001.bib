@article{ WOS:001144008600001,
Author = {Karpowitz, Christopher f. and Monson, J. quin and Preece, Jessica r. and
   Aldridge, Alejandra},
Title = {Selecting for Masculinity: Women's Under-Representation in the
   Republican Party},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2024},
Volume = {118},
Number = {4},
Pages = {1873-1894},
Month = {NOV},
Abstract = {The gap between women's representation in the Democratic and Republican
   parties has grown significantly in the last three decades. We argue
   existing explanations undervalue voters' contributions to this trend by
   focusing on voter responses to candidate sex rather than candidate
   gender. We theorize that Republican voters (especially the most
   conservative) prefer masculine candidates in intraparty and entry-level
   elections. Because sex and gender are correlated, this limits the number
   of Republican women who advance through the political pipeline.
   Experimental vignettes from two rounds of the Cooperative Congressional
   Election Study (N = 2,000) and two large surveys of Republicans (N >
   10,000) show that Republican (but not Democratic) voters penalize
   candidates with ``feminine{''} self-presentation regardless of the
   candidate's sex. Original data on the self-presentation of Republican
   candidates for entry-level office (N = 459) confirm Republican
   candidates often present themselves in gender-stereotypical ways. In
   short, voters play an underappreciated role in the partisan gap in
   women's representation.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {EDINBURGH BLDG, SHAFTESBURY RD, CB2 8RU CAMBRIDGE, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Preece, JR (Corresponding Author), Brigham Young Univ, Dept Polit Sci, Provo, UT 84602 USA.
