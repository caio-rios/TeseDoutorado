@article{ WOS:001283755000001,
Author = {Valdez, Ines and Rocco, Raymond and Chang, Arturo},
Title = {Tradition and Disruption in Latinx and Latin American Political Thought},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2024},
Month = {2024 AUG 5},
Abstract = {This article presents Latinx political thought as a distinctive
   tradition in political theory that reworks central concepts in response
   to historical experiences of conquest, colonialism, migration, and
   transnational politics. In reconstructing this tradition, we argue that
   its problem space converges with US-based Latin American political
   thought. The article first traces a genealogy of Latinx political theory
   and then explores three realms of theorizing around which Latinx and
   Latin American political thought cluster: sovereignty and state
   violence, peoplehood, and transnationalism. We explain how the surveyed
   works disrupt and enrich political theory accounts of these problems. In
   arguing for the recognition of this field as a tradition, the article
   also aims to make it intelligible as an area of concentration for PhD
   students in political science.},
