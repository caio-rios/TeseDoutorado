@article{ WOS:A1996UN26900002,
Author = {Bennett, DS and Stam, AC},
Title = {The duration of interstate wars, 1816-1985},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1996},
Volume = {90},
Number = {2},
Pages = {239-257},
Month = {JUN},
Note = {28th Annual Meeting of the Peace-Science-Society, UNIV ILLINOIS,
   CHAMPAIGN, IL, NOV 04-06, 1994},
Organization = {Peace Sci Soc},
Abstract = {We present a model of war duration which incorporates both realpolitik
   and domestic political variables. We hypothesize that strategy, terrain,
   capabilities, and government type, among other variables, will play key
   roles in determining the duration of war. We test these hypotheses using
   hazard analysis and find empirical support for our key arguments. We
   find that the realpolitik variables play a greater role than regime
   behavior and type in determining war duration. We also find that
   historically, on average, mobilization and strategic surprise have
   little effect on war duration and that wars are not duration dependent.},
