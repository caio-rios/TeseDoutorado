@article{ WOS:000458492100013,
Author = {Lesch, Charles H. T.},
Title = {Theopolitics Contra Political Theology: Martin Buber's Biblical Critique
   of Carl Schmitt},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
