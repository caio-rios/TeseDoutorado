@article{ WOS:000395488200007,
Author = {Invernizzi-Accetti, Carlo and Wolkenstein, Fabio},
Title = {The Crisis of Party Democracy, Cognitive Mobilization, and the Case for
   Making Parties More Deliberative},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2017},
Volume = {111},
Number = {1},
Pages = {97-109},
Month = {FEB},
Abstract = {The much-discussed crisis of political parties poses a challenge to
   democratic theorists as institutional designers: how can the capacity of
   parties to mediate between society and state be resuscitated? In this
   article, we suggest that parties need to become more internally
   deliberative, allowing partisans to debate policy and more general
   visions for the polity. We outline a prescriptive model of deliberative
   intraparty democracy, drawing on the empirical literature on the
   changing structure of civic and political engagement. We argue that
   deliberative reforms are the most appropriate response to the demands of
   an increasingly more cognitively mobilized citizenry which seeks
   self-expression and nonhierarchical forms of political engagement. We
   highlight the model's distinctive strengths and defend it against
   several objections.},
