@article{ WOS:000446999100013,
Author = {Gourevitch, Alex},
Title = {The Right to Strike: A Radical View},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2018},
Volume = {112},
Number = {4},
Pages = {905-917},
Month = {NOV},
Abstract = {Workers face a common dilemma when exercising their right to strike. For
   the worst-off workers to go on strike with some reasonable chance of
   success, they must use coercive strike tactics like mass pickets and
   sit-downs. These tactics violate some basic liberties, such as contract,
   association, and private property, and the laws that protect those
   liberties. Which has priority, the right to strike or the basic
   liberties strikers might violate? The answer depends on why the right to
   strike is justified. In contrast to liberal and social democratic
   arguments, on the radical view defended here, the right to strike is a
   right to resist oppression. This oppression is partly a product of the
   legal protection of basic economic liberties, which explains why the
   right to strike has priority over these liberties. The radical view thus
   best explains why workers may use some coercive, even lawbreaking,
   strike tactics.},
