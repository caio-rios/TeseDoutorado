@article{ WOS:000323275400005,
Author = {Cohen, Dara Kay},
Title = {Explaining Rape during Civil War: Cross-National Evidence (1980-2009)},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2013},
Volume = {107},
Number = {3},
Pages = {461-477},
Month = {AUG},
Abstract = {Why do some armed groups commit massive wartime rape, whereas others
   never do? Using an original dataset, I describe the substantial
   variation in rape by armed actors during recent civil wars and test a
   series of competing causal explanations. I find evidence that the
   recruitment mechanism is associated with the occurrence of wartime rape.
   Specifically, the findings support an argument about wartime rape as a
   method of socialization, in which armed groups that recruit by
   force-through abduction or pressganging-use rape to create unit
   cohesion. State weakness and insurgent contraband funding are also
   associated with increased wartime rape by rebel groups. I examine
