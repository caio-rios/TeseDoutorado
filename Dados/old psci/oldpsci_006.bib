@article{ WOS:000297508000002,
Author = {Evans, Laura E.},
Title = {Expertise and Scale of Conflict: Governments as Advocates in American
   Indian Politics},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2011},
Volume = {105},
Number = {4},
Pages = {663-682},
Month = {NOV},
Abstract = {How do American Indian tribal governments relate to nearby local
   governments? Do insights gleaned from these cases illuminate the
   constraints and opportunities that marginalized groups face within any
   system of federalism? What circumstances of marginalized governments
   help or hinder their effectiveness? Although some Native American tribes
   have transformed their fortunes with highly profitable casinos, most
   continue to face stark disadvantages. Some tribal governments, despite
   limited opportunities prevail locally by cultivating policy and
   political expertise. This analysis demonstrates that such expertise can
   be developed, even when resources are scarce.},
