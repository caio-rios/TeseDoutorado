@article{ WOS:000315340600001,
Author = {Hegghammer, Thomas},
Title = {Should I Stay or Should I Go? Explaining Variation in Western Jihadists'
   Choice between Domestic and Foreign Fighting},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
