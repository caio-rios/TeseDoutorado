@article{ WOS:000172958300003,
Author = {Wand, JN and Shotts, KW and Sekhon, JS and Mebane, WR and Herron, MC and
   Brady, HE},
Title = {The butterfly did it: The aberrant vote for Buchanan in Palm Beach
   County, Florida},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2001},
Volume = {95},
Number = {4},
Pages = {793-810},
Month = {DEC},
Note = {Annual Meeting of the Public-Choice-Society, SAN ANTONIO, TEXAS, MAR
   09-11, 2001},
Organization = {Publ Choice Soc},
Abstract = {We show that the butterfly ballot used in Palm Beach County, Florida, in
   the 2000 presidential election caused more than 2,000 Democratic voters
   to vote by mistake for Reform candidate Pat Buchanan, a number larger
   than George W. Bush's certified margin of victory in Florida. We use
   multiple methods and several kinds of data to rule out alternative
   explanations for the votes Buchanan received in Palm Beach County. Among
   3,053 U.S. counties where Buchanan was on the ballot, Palm Beach County
   has the most anomalous excess of votes for him. In Palm Beach County,
   Buchanan's proportion of the vote on election-day ballots is four times
   larger than his proportion on absentee (nonbutterfly) ballots, but
   Buchanan's proportion does not differ significantly between election-day
   and absentee ballots in any other Florida county. Unlike other Reform
   candidates in Palm Beach County, Buchanan tended to receive election-day
   votes in Democratic precincts and fi-om individuals who voted for the
   Democratic U.S. Senate candidate. Robust estimation of overdispersed
   binomial regression models underpins much of the analysis.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {40 WEST 20TH ST, NEW YORK, NY 10011-4221 USA},
Type = {Article; Proceedings Paper},
Language = {English},
Affiliation = {Wand, JN (Corresponding Author), Cornell Univ, Dept Govt, Ithaca, NY USA.
   Cornell Univ, Dept Govt, Ithaca, NY USA.
   Lamarck Inc, Boston, MA 02116 USA.
