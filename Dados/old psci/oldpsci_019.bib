@article{ WOS:A1995QJ64500009,
Author = {CLINGERMAYER, JC and WOOD, BD},
Title = {DISENTANGLING PATTERNS OF STATE DEBT FINANCING},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1995},
Volume = {89},
Number = {1},
Pages = {108-120},
Month = {MAR},
Abstract = {We examine the determinants of change in state government indebtedness
   from 1961 through 1989 using a pooled time series cross-sectional
   analysis. The analysis reveals that debt is primarily a function of
   economic conditions reflecting both the need to borrow and the capacity
   of states to repay debt. However, political factors such as culture,
   partisan competition, and electoral cycles also affect state debt. We
   also find very weak evidence that tax and expenditure limitations,
   ironically, may increase state indebtedness, while constitutional debt
   limitations have no effect upon slowing the growth of state debt.},
