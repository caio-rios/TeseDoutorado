@article{ WOS:000181293900007,
Author = {Wellhofer, ES},
Title = {Democracy and fascism: Class, civil society, and rational choice in
   Italy},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2003},
Volume = {97},
Number = {1},
Pages = {91-106},
Month = {FEB},
Note = {European Consortium for Political Research Workshops, TURIN, ITALY, MAR
   22-28, 2002},
Abstract = {The origins of fascism remain a major concern to social scientists.
   Because fascism emerged in societies seeking transitions to democracy, a
   better understanding of these failed attempts at democratic transitions
   improves our understanding of both democracy's possibilities and the
   strengths and weakness of democratic theory. Indeed, theoretical
   arguments employed to explain fascism have their analogues in theories
   of democracy. Three arguments have been advanced to explain both
   democracy and fascism: class, civil society, and rational choice. This
   research examines the rise of fascism in Italy, 1919-21. The evidence
   contradicts the class theory of fascism and offers mixed evidence for
   the civil society theory, while supporting the rational choice theory.},
Publisher = {CAMBRIDGE UNIV PRESS},
Address = {40 WEST 20TH ST, NEW YORK, NY 10011-4221 USA},
