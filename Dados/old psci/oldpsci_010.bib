@article{ WOS:000185098500006,
Author = {Korpi, W and Palme, J},
Title = {New politics and class politics in the context of austerity and
   globalization: Welfare state regress in 18 countries, 1975-95},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
