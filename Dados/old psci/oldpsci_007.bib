@article{ WOS:000264926400006,
Author = {Keefer, Philip and Khemani, Stuti},
Title = {When Do Legislators Pass on Pork? The Role of Political Parties in
   Determining Legislator Effort},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2009},
Volume = {103},
Number = {1},
Pages = {99-112},
Month = {FEB},
Abstract = {A central challenge in political economy is to identify the conditions
   under which legislators seek to ``bring home the pork{''} to
   constituents. We conduct the first systematic analysis of one
   determinant of constituency service, voter attachment to political
   parties, holding constant electoral and political institutions. Our
   analysis takes advantage of data from a unique type of public spending
   program that is proliferating across developing countries, the
   constituency development fund (CDF), which offers more precise measures
   of legislator effort than are common in the literature. Examining the
   CDF in India, we find that legislator effort is significantly lower in
   constituencies that are party strongholds. This result, which is robust
   to controls for alternate explanations, implies that legislators pass on
   pork when voters are more attached to political parties. It has
   implications not only for understanding political incentives and the
   dynamics of partly, formation, but also for evaluating the impact of
   CDFs.},
