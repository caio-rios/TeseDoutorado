@article{ WOS:000079116700001,
Author = {Jennings, MK},
Title = {Political responses to pain and loss - Presidential address, American
   Political Science Association, 1998},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1999},
Volume = {93},
Number = {1},
Pages = {1-13},
Month = {MAR},
Note = {Warren E Miller and Philip E Converse Annual Lecture Series, ANN ARBOR,
   MICHIGAN, APR, 1998},
Abstract = {Although manifestations of pain and loss phenomena are treated in
   various parts of the discipline, the focus is seldom on pain and loss as
   a distinctive form of political experience or as one that offers a broad
   canvas on which the workings of the political process can be depicted.
   By contrast, this article makes four arguments: (I) pain and loss
   experiences cut to the core of everyday lives and frequently infuse them
   with politics; (2) responses to pain and loss events occupy a prominent
   place in the domains of public opinion and issue activism; (3) these
   events and responses have some unique properties; and (4) major research
   questions can be organized around the study of pain and loss phenomena.},
