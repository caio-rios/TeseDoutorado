@article{ WOS:000082193500008,
Author = {Smith, TW},
Title = {Aristotle on the conditions for and limits of the common good},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1999},
Volume = {93},
Number = {3},
Pages = {625-636},
Month = {SEP},
Abstract = {Contemporary debates over liberal political theory should encourage
   renewed investigation of the common good, and it is appropriate to begin
   ty interrogating Aristotle's account. Aristotle argues that injustice
   stands in the way of the common good. Injustice is motivated by both
   overgrasping for scarce external goods, such as money, honor, and power;
   and by excessive desires. Aristotle argues that the common good requires
   a reorientation away from external goods to satisfying activities that
   do nor diminish in the sharing. He sh-etches an analogical account of
   familial and political relationships that leads us to wonder what the
   political conditions are for the common good. Reflecting on these
   conditions not only points to the strict limits of the common good but
   also speaks to both sides in debates over liberal theory.},
