@article{ WOS:000080486900005,
Author = {Hetherington, MJ},
Title = {The effect of political trust on the presidential vote, 1968-96},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1999},
Volume = {93},
Number = {2},
Pages = {311-326},
Month = {JUN},
Note = {Annual Meeting of the Midwest-Political-Science-Association, CHICAGO,
   IL, APR 10-12, 1997},
Organization = {Midwest Polit Sci Assoc},
Abstract = {Scholars hale consistently demonstrated that no link exists between
   declining political trust and declining armour, but they have paid less
   attention to the effect of trust on vote choice. In an Era characterized
   by declining trust, the incumbent party has lost, and third parties have
   strongly contested, four of the last eight presidential elections. Such
   outcomes are historically anomalous. This study demonstrates that
   declining political trust affects vote choice, but the electoral
   beneficiary differs according to electoral context, lit two-candidate
   races, politically distrustful voters support candidates from the
   nonincumbent major party. In races with three viable candidates,
   third-party alternatives benefit front declining political mist at the
   expense of both major parties.},
Publisher = {CAMBRIDGE UNIV PRESS},
