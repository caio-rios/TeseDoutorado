@article{ WOS:000079116700013,
Author = {Lublin, D},
Title = {Racial redistricting and African-American representation: A critique of
   ``Do majority-minority districts maximize substantive black
   representation in congress?{''}},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1999},
Volume = {93},
Number = {1},
Pages = {183-186},
Month = {MAR},
Abstract = {Contrary to Cameron, Epstein, and O'Halloran's article (1996), (1)
   racial redistricting remains vital to the election of African Americans
   to the U.S. House, and (2) the tradeoff between black descriptive and
   substantive representation is actually greater in the South than in the
   North. Substantive and methodological errors explain why they arrived at
   their findings. Specifically, their analysis ignores the effect of the
   presence of Latinos on the election of African Americans. Ironically,
   due to the very policy assessed in the article, Cameron, Epstein, and
   O'Halloran's data set does not allow them to examine the link between
   the racial composition of a district and the ideology of its
   representative. In addition, they do not consider that racial
   redistricting not only changes the aggregation of seats into votes but
   also indirectly boosts the Republican share of votes and seats.},
