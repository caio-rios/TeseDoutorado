@article{ WOS:000077384000011,
Author = {Erikson, RS and Mackuen, MB and Stimson, JA},
Title = {What moves macropartisanship? A response to Green, Palmquist, and
   Schickler},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1998},
Volume = {92},
Number = {4},
Pages = {901-912},
Month = {DEC},
Abstract = {Contrary to the claim by Green, Palmquist, and Schickler (1998),
   macropartisanship is largely shaped by presidential approval and
   consumer sentiment. It is not the case, however, that macropartisanship
   mirrors the ever-changing levels of current presidential popularity and
   prosperity Rather, macropartisanship reflects the cumulation of
   political and economic news that shapes approval and consumer sentiment.
   Using ECM technology, we show that, far from being the weak force that
   Green et al. suggest, the cumulation of innovations in presidential
   approval and consumer sentiment largely account for the long-term trends
   in macropartisanship. For forecasting macropartisanship in the near
   future, it is better to predict from the fundamentals represented by the
   history of approval and consumer sentiment up to a given moment than
   from current values of macropartisanship itself.},
