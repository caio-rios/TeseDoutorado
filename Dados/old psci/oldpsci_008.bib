@article{ WOS:000248817900001,
Author = {Cusack, Thomas R. and Iversen, Torben and Soskice, David},
Title = {Economic interests and the origins of electoral systems},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2007},
Volume = {101},
