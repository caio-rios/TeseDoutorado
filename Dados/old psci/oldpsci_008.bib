@article{ WOS:000242682200009,
Author = {Wald, Kenneth D. and Wilcox, Clyde},
Title = {Getting religion: Has political science rediscovered the faith factor?},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2006},
Volume = {100},
Number = {4},
Pages = {523-529},
Month = {NOV},
Abstract = {To judge by the absence of religion from the pages of the American
   Political Science Review in its first century, most political scientists
   have embraced a secular understanding of the political world. We explore
   the evolving status of religion in the discipline by examining patterns
   of scholarly inquiry in the discipline's flagship journal. After finding
   religion an (at best) marginal topic and rejecting some plausible
