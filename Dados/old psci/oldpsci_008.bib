@article{ WOS:000253697800001,
Author = {Merrill, III, Samuel and Grofman, Bernard and Brunell, Thomas L.},
Title = {Cycles in American national electoral politics, 1854-2006: Statistical
   evidence and an explanatory model},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2008},
Volume = {102},
Number = {1},
Pages = {1-17},
