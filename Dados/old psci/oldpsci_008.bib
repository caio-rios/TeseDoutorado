@article{ WOS:000257183500003,
Author = {Martin, Cathie Jo and Swank, Duane},
Title = {The political origins of coordinated capitalism: Business organizations,
   party systems, and state structure in the age of innocence},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2008},
Volume = {102},
Number = {2},
Pages = {181-198},
Month = {MAY},
Abstract = {This paper investigates the political determinants of corporatist and
   pluralist employers' associations and reflects on the origins of the
   varieties of capitalism in the early decades of the 20th century. We
   hypothesize that proportional, multiparty systems tend to enable
   employers' associations to develop into social corporatist
   organizations, whereas nonproportional, two-party systems are conducive
