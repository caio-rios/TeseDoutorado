@article{ WOS:000259787100006,
Author = {Bailey, Michael A. and Maltzman, Forrest},
Title = {Does legal doctrine matter? Unpacking law and policy preferences on the
   US Supreme Court},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2008},
Volume = {102},
Number = {3},
Pages = {369-384},
Month = {AUG},
Abstract = {Judicial scholars often struggle to disentangle the effects of law and
   policy preferences on US Supreme Court decision making. We employ a new
   approach to measuring the effect-if any-of the law on justices'
   decisions. We use positions taken on Supreme Court cases by members of
   Congress and presidents to identify policy components of voting. Doing
   so enables us to isolate the effects of three legal doctrines: adherence
   to precedent, judicial restraint, and a strict interpretation of the
   First Amendment's protection of speech clause. We find considerable
   evidence that legal factors play an important role in Supreme Court
   decision making. We also find that the effect of legal factors varies
   across justices.},
