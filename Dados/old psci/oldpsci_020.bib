@article{ WOS:A1993LV44600001,
Author = {SMITH, RM},
Title = {BEYOND TOCQUEVILLE, MYRDAL, AND HARTZ - THE MULTIPLE TRADITIONS IN
   AMERICA},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1993},
Volume = {87},
Number = {3},
Pages = {549-566},
Month = {SEP},
Abstract = {Analysts of American politics since Tocqueville have seen the nation as
   a paradigmatic `'liberal democratic'' society, shaped most by the
   comparatively free and equal conditions and the Enlightenment ideals
   said to have prevailed at its founding. These accounts must be severely
   revised to recognize the inegalitarian ideologies and institutions of
   ascriptive hierarchy that defined the political status of racial and
   ethnic minorities and women through most of U.S. history. A study of the
   period 1870-1920 illustrates that American political culture is better
   understood as the often conflictual and contradictory product of
   multiple political traditions, than as the expression of hegemonic
   liberal or democratic political traditions.},
