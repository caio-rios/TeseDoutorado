@article{ WOS:A1994NB94900006,
Author = {MEBANE, WR},
Title = {FISCAL CONSTRAINTS AND ELECTORAL MANIPULATION IN AMERICAN SOCIAL-WELFARE},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1994},
Volume = {88},
Number = {1},
Pages = {77-94},
Month = {MAR},
Abstract = {American social welfare policy involves a politically significant fiscal
   interaction between short-run, pay-as-you-go constraints and long-run
   equilibrium constraints motivated by the contributory principle and the
   concept of actuarial soundness. The fiscal constraints induce conflict
   between benefit recipients and payroll-tax-payers. To support small
   election-year reductions in payroll taxes, means-tested program benefits
   are slightly reduced in election years (compared to non-election-year
   levels under similar economic conditions). The existence of the
   constraints and of the election-year changes is tested using a
