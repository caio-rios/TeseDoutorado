@article{ WOS:000609155000005,
Author = {Charette, Danielle},
Title = {David Hume's Balancing Act: The <i>Political Discourses</i> and the
   Sinews of War},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
