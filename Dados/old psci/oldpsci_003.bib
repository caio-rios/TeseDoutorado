@article{ WOS:000553658900013,
Author = {Luhrmann, Anna and Marquardt, Kyle L. and Mechkova, Valeriya},
Title = {Constraining Governments: New Indices of Vertical, Horizontal, and
   Diagonal Accountability},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2020},
Volume = {114},
Number = {3},
Pages = {811-820},
Month = {AUG},
Abstract = {Accountability-constraints on a government's use of political power-is
   one of the cornerstones of good governance. However, conceptual
   stretching and a lack of reliable measures have limited cross-national
   research on this concept. To address this research gap, we use V-Dem
   data and innovative Bayesian methods to develop new indices of
   accountability and its subtypes: the extent to which governments are
   accountable to citizens (vertical accountability), other state
   institutions (horizontal accountability), and the media and civil
   society (diagonal accountability). In this article, we describe the
   conceptual and empirical framework underlying these indices and
   demonstrate their content, convergent, and construct validity. The
   resulting indices have unprecedented coverage (1900-present) and offer
   researchers and policymakers new opportunities to investigate the causes
   and consequences of accountability and its disaggregated subtypes.
   Furthermore, the methodology provides a framework for theoretically
   driven index construction to scholars working with cross-national panel
   data.},
