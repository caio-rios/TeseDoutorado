@article{ WOS:000075532800007,
Author = {Baron, DP},
Title = {Comparative dynamics of parliamentary governments},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1998},
Volume = {92},
Number = {3},
Pages = {593-609},
Month = {SEP},
Abstract = {This article presents a dynamic theory of parliamentary governments that
   incorporates attributes of the institutional system in a country,
   exogenous events that shape parliamentary and electoral opportunities,
   and the strategies of the government and the opposition as structured by
