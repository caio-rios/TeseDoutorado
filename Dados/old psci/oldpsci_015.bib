@article{ WOS:000077384000008,
Author = {Kato, J},
Title = {When the party breaks up: Exit and voice among Japanese legislators},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1998},
Volume = {92},
Number = {4},
Pages = {857-870},
Month = {DEC},
Note = {Conference on Democratic Institutions in East Asia, DUKE UNIV, DURHAM,
   NORTH CAROLINA, NOV 08-09, 1996},
Abstract = {In exploring the conflicts between individual interests and the
   conditions that facilitate the disintegration of a political party, this
   article modifies the exit, voice, and loyalty framework developed by
   Hirschman. The utility of that framework is examined using the recent
   change in Japan, that is, the demise of the so-called 1955 system in
   which the predominant conservatives confined the socialists to the
   status of a major but perennial opposition party. The quantitative
   analysis focuses on the split of the Japanese Liberal Democratic Party
   and the internal dispute in the Democratic Socialist party of Japan.
   These cases provide an interesting comparison of how individual
   characteristics and contextual conditions affect members' decisions to
   exercise exit and voice. At the same time, they illuminate how
   party-level changes have been influenced by intraparty factors.},
