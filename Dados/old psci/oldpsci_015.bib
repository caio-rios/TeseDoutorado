@article{ WOS:000075532800010,
Author = {Forde, S},
Title = {Hugo Grotius on ethics and war},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1998},
Volume = {92},
Number = {3},
Pages = {639-648},
Month = {SEP},
Abstract = {Interest in the thought of Hugo Grotius on international law and ethics
   is justified inasmuch as he attempted to define a theoretical position
   between an idealism he thought counterproductive and an amoral realism
   he found unacceptable. Grotius constructed a system in which the moral
   authority of natural law was combined with the flexibility of human law.
   This required him to develop a special understanding of the nature and
   relation of these two types of law. In giving the law of nations, as a
   product of human will, the authority to suspend provisions of natural
   law he provided for a code of international conduct that could permit
   injustice where necessary, without abandoning moral ideals altogether.},
