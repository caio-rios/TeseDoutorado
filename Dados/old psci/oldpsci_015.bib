@article{ WOS:000073952900001,
Author = {Saxonhouse, AW},
Title = {Democracy, equality, and Eide: A radical view from book 8 of Plato's
   Republic},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1998},
Volume = {92},
Number = {2},
Pages = {273-283},
Month = {JUN},
Abstract = {A Plate opposed to democracy fills the literature, and while some
   scholars question whether Plate adequately captures Socrates' possibly
   favorable views of democracy, Plate himself remains a paragon of
   elitism. I argue that Plate's response to democracy is far more
   theoretically interesting than simple disdain for the unenlightened
   masses. Rather, In Book 8 of the Republic he explores the fundamental
   tensions of a regime identified with freedom and equality, which he
   presents as characterized by formlessness, and the epistemological and
   theoretical problems posed by the absence of forms (eide). Eide give
   structure and identity to regimes and to their citizens; they are
   necessary for intellection and philosophy, but they are also the grounds
   for compulsion. Plate's analysis of democracy thus becomes a more
   serious challenge for democratic theorists than previous recognized.},
