@article{ WOS:000231547800011,
Author = {Slantchev, BL and Alexandrova, A and Gartzke, E},
Title = {Probabilistic causality, selection bias, and the logic of the democratic
   peace},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2005},
Volume = {99},
Number = {3},
Pages = {459-462},
Month = {AUG},
Abstract = {Rosato (2003) claims to have discredited democratic peace theories.
   However, the methodological approach adopted by the study cannot
   reliably generate the conclusions espoused by the author. Rosato seems
   to misunderstand the probabilistic nature of most arguments about
   democratic peace and ignores issues that an appropriate research design
   should account for. Further, the study's use of case studies and data
   sets without attention to selection-bias produces examples that actually
   support theories it seeks to undermine. These problems place in doubt
   the article's findings.},
