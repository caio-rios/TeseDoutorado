@article{ WOS:000231547800003,
Author = {Meguid, BM},
Title = {Competition between unequals: The role of mainstream party strategy in
   niche party success},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2005},
Volume = {99},
Number = {3},
Pages = {347-359},
Month = {AUG},
Note = {60th Annual Meeting of the Midwest-Political-Science-Association,
   CHICAGO, IL, APR 25-28, 2002},
Organization = {Midwest Polit Sci Assoc},
Abstract = {What accounts for variation in the electoral success of niche parties?
   Although institutional and sociological explanations of single-issue
   party strength have been dominant, they tend to remove parties from the
   analysis. In this article, I argue that the behavior of mainstream
   parties influences the electoral fortunes of the new, niche party
   actors. In contrast to standard spatial theories, my theory recognizes
   that party tactics work by altering the salience and ownership of issues
   for political competition, not just party issue positions. It follows
   that niche party support can be shaped by both proximal and non-proximal
   competitors. Analysis of green and radical right party vote in 17
   Western European countries from 1970 to 2000 confirms that mainstream
   party strategies matter; the modified spatial theory accounts for the
   failure and success of niche parties across countries and over time
   better than institutional, sociological, and even standard spatial
   explanations.},
