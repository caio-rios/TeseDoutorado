@article{ WOS:000223700800004,
Author = {Duch, RM and Palmer, HD},
Title = {It's not whether you win or lose, but how you play the game:
   Self-interest, social justice, and mass attitudes toward market
   transition},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {2004},
Volume = {98},
Number = {3},
Pages = {437-452},
Month = {AUG},
Note = {97th Annual Meeting of the American-Political-Science-Association, SAN
   FRANCISCO, CA, AUG 29-SEP 02, 2001},
Organization = {Amer Polit Sci Assoc},
Abstract = {To explore systematic differences in economic reasoning and what might
   account for them, we investigate how sociocultural conditions affect
   transitions to market economies in the West African country of Benin. We
   probe the importance of several factors: basic economic norms, utility
   maximization behavior, individual-level personal capital, and
   individual-level social capital. The evidence, based on experiments
   embedded in an opinion survey, indicates that Beninese citizens widely
   share commitments to the basic foundations of economic interaction,
   e.g., property rights. The nature of social capital varies across
   cultural and political contexts and accounts for cross-contextual
   variation in the costs associated with cooperative behavior and in
   utility maximization behavior.},
