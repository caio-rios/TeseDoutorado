@article{ WOS:A1992JP20200013,
Author = {WARD, MD and DAVIS, DR},
Title = {SIZING UP THE PEACE DIVIDEND - ECONOMIC-GROWTH AND MILITARY SPENDING IN
   THE UNITED-STATES, 1948-1996},
Journal = {AMERICAN POLITICAL SCIENCE REVIEW},
Year = {1992},
Volume = {86},
Number = {3},
Pages = {748-755},
Month = {SEP},
Abstract = {We examine the relationship between military expenditures and economic
   growth in the United States from  1948 to 1990, in order to gauge the
   potential peace dividend. Our main results suggest that military
   spending is a significant drain on the economy. We then examine the
   implications of a restructured international system for U.S. military
   expenditures and their resultant impact on economic growth in the 1990s.
   Simulations of Democratic and Republican proposals for cuts in defense
   spending suggest increases in economic output of between 2.5\% and 4.5\%
   over the period 1993-96.},
