@article{ WOS:000323285300004,
Author = {Weinstein, Liza},
Title = {Demolition and Dispossession: Toward an Understanding of State Violence
   in Millennial Mumbai},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
Year = {2013},
Volume = {48},
