@article{ WOS:000320105900001,
Author = {Barnes, Tiffany D. and Burchard, Stephanie M.},
Title = {``Engendering{''} Politics: The Impact of Descriptive Representation on
   Women's Political Engagement in Sub-Saharan Africa},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2013},
Volume = {46},
Number = {7},
Pages = {767-790},
Month = {JUL},
Abstract = {Globally, there is a significant gender gap in political engagement
   between men and women; however, this gender gap varies both across
   countries and within countries over time. Previous research has argued
   that the inclusion of women in elite political positions encourages
   women's political engagement at the citizen levelby augmenting women's
   symbolic representationand can reduce this gender gap. Using
   Afrobarometer data from 20 African countries across four waves of
   surveys from 1999 to 2008, we employ an interactive multilevel model
   that controls for the sex of the respondent, the percentage of women in
   the legislature, and the interaction of these two variables. We find
   that as women's descriptive representation increases, the political
   engagement gender gap diminishes. This finding is robust across several
   measures of political engagement. Our findings suggest that the
   incorporation of women into political institutions encourages the
   political engagement of women at the citizen level.},
