@article{ WOS:000321057300004,
Author = {Bowling, Cynthia J. and Pickerill, J. Mitchell},
Title = {Fragmented Federalism: The State of American Federalism 2012-13},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2013},
Volume = {43},
Number = {3, SI},
Pages = {315-346},
Month = {SUM},
Abstract = {Intense party polarization at the state and federal levels has hindered
   the adoption and implementation of numerous policies. One of the most
   important ways we see ``fragmented federalism{''} is in the patchwork of
   policies across the country, created by factions and implemented in
   fragments across and within conflicted institutions throughout 2012-13.
   Implementation of various policies-notably education and health care-has
   been plagued by partisanship, pushback, and uncertainty. Operating under
   conditions of fiscal stress and a federal budget plagued by roadblocks
   has further complicated planning and program implementation at all
   government levels. Finally, Supreme Court decisions continue to
   contribute to the fragmented nature of federalism by alternately
   protecting state sovereignty from federal intrusion and at other times
   endorsing broad federal preemption of state laws.},
