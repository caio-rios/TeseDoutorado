@article{ WOS:000308880400004,
Author = {De Francesco, Fabrizio},
Title = {Diffusion of Regulatory Impact Analysis Among OECD and EU Member States},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2012},
Volume = {45},
Number = {10},
Pages = {1277-1305},
Month = {OCT},
Abstract = {With the exception of few comparative case studies, the literature on
   regulatory reform and regulatory impact analysis (RIA) tends to focus on
   internal political actors, activities, and processes. Furthermore,
   empirical analyses of new public management have overlooked the dynamics
   of communications among networks of administrative reformers. This
   article fills these gaps, presenting results of an event history
   analysis on the diffusion of RIA. It probes rationales for the origin of
   RIA and administrative capacity explanations in combination with
   variables referring to international and transnational communication
   channels of administrative reforms. A hypothesis based on legal origin
   is also tested. The findings show that the decision to adopt RIA rests
   on transnational networks as well as administrative variables such as
   government expenditure and legal origin.},
