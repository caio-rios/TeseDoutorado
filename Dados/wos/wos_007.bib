   Columbia Univ, Ctr Democracy Tolerat \& Relig, New York, NY 10027 USA.},
DOI = {10.5129/001041512801282960},
ISSN = {0010-4159},
EISSN = {2151-6227},
Keywords-Plus = {POLITICS; POLICY},
