@article{ WOS:000311519800003,
Author = {Karakoc, Ekrem and Baskan, Birol},
Title = {Religion in Politics: How Does Inequality Affect Public Secularization?},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2012},
Volume = {45},
Number = {12},
Pages = {1510-1541},
Month = {DEC},
Abstract = {This study investigates the factors that affect variations in secular
   attitudes toward politics. The literature suggests that modernization
   may weaken traditional bonds with religious adherence and the state can
   assume an important role in this endeavor through mass education,
   industrialization, and other factors. However, this explanation is
   incomplete in light of the resurgence of religious movements. This study
   argues that economic inequality increases the positive evaluation of the
   role of religion in politics through its effect on religiosity and
   participation in religious organizations. Employing a multilevel
   analysis on 40 countries, this study demonstrates that inequality
   decreases attitudes toward support for two dimensions of public
   secularization: the secularization of public office holders and the
   influence of religious leaders in politics. Simultaneously, the effect
   of modernization on these attitudes varies. The results also suggest
   that although inequality diminishes secular attitudes of all
   socioeconomic groups, its effect is nonlinear, with a greater effect on
   the poor.},
