@article{ WOS:000306373700006,
Author = {Chernick, Howard and Langleyy, Adam and Reschovsky, Andrew},
Title = {Predicting the Impact of the U.S. Housing Crisis and ``Great
   Recession{''} on Central City Revenues},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2012},
Volume = {42},
Number = {3, SI},
Pages = {467-493},
Month = {SUM},
Abstract = {Using data from 1983 to 2009 on revenues, housing prices, and incomes,
   we forecast the impact of the U.S. housing crisis and ``Great
   Recession{''} on the revenues of the nation's largest central cities for
   2009-2013. We develop the concept of constructed cities, to allow valid
   comparisons across cities in light of varying expenditure
   responsibilities of city governments. We predict that general revenue
   per capita in the average central city will decline in real terms over
   the 4-year period by 3.5 percent. The largest revenue reductions occur
   in constructed cities where housing prices fell substantially and state
   aid was sharply reduced.},
