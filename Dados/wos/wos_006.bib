@article{ WOS:000359264700003,
Author = {Naoi, Megumi and Kume, Ikuo},
Title = {Workers or Consumers? A Survey Experiment on the Duality of Citizens'
   Interests in the Politics of Trade},
Journal = {COMPARATIVE POLITICAL STUDIES},
