@article{ WOS:000357764600006,
Author = {Sorace, Christian},
Title = {The Communist Party's Miracle? The Alchemy of Turning Post-Disaster
   Reconstruction into Great Leap Development},
Journal = {COMPARATIVE POLITICS},
Year = {2015},
Volume = {47},
Number = {4},
Pages = {479+},
Month = {JUL},
Abstract = {The May 12th, 2008 Sichuan earthquake was a national trauma in China.
   The reconstruction provided the Party with an opportunity to display its
   care for the disaster victims and restore the Party's shaken credibility
   and socialist legitimacy. Despite initial collective solidarity and firm
   control over the state apparatus, levers of the economy, and domestic
   media, the Party did not manage to secure broad public approval of its
   reconstruction effort in the earthquake zone. This article argues that
   the reasons for this failure can be traced to the Party's political
   epistemology. The CCP's general assumptions, governmental rationalities,
   policy calculations, implementation strategies, and legitimating
   discourses organized the reconstruction. It wanted to build a model of
   state power and benevolence, but it did not have the proper tools.},
