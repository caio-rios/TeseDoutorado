@article{ WOS:000343698600009,
Author = {Lecours, Andre},
Title = {The Question of Federalism in Nepal},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2014},
Volume = {44},
Number = {4},
Pages = {609-632},
Month = {FAL},
Abstract = {In 2012, a Nepali Constituent Assembly tasked with drafting a federal
   constitution was dissolved after four years of wrangling over federalism
   issues. This article develops three explanations for why federal
   structures have yet to take shape in Nepal. It argues that consensus on
   federalism hides a reluctance by key actors to build a federal system;
   that while some political forces want federal structures based on ethnic
   identities, two of the three main political parties have little appetite
   for ``identity-based federalism''; and that political actors hold
   antagonistic ideas about federalism and what it should achieve. More
   broadly, the article speaks to cases of ``holding together'' federalism
   stemming from previously unitary states.},
Publisher = {OXFORD UNIV PRESS},
Address = {GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Lecours, A (Corresponding Author), Univ Ottawa, Ottawa, ON K1N 6N5, Canada.
