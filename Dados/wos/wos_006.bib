@article{ WOS:000372406900002,
Author = {Lankina, Tomila V. and Libman, Alexander and Obydenkova, Anastassia},
Title = {APPROPRIATION AND SUBVERSION Precommunist Literacy, Communist Party
   Saturation, and Postcommunist Democratic Outcomes},
Journal = {WORLD POLITICS},
