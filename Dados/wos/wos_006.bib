@article{ WOS:000349320200002,
Author = {Carlin, Ryan E. and Love, Gregory J. and Martinez-Gallardo, Cecilia},
Title = {Security, Clarity of Responsibility, and Presidential Approval},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2015},
Volume = {48},
Number = {4},
Pages = {438-463},
Month = {MAR},
Abstract = {The importance of institutions in shaping citizens' ability to punish or
   reward politicians for economic outcomes is well established. Where
   institutions divide authority, politicians can blame each other and
   citizens find it harder to assign responsibility for policy failures;
   where institutions clarify lines of authority, citizens can better hold
   politicians accountable. However, this argument assumes that citizens
   perceive policy responsibility as shared among political actors and this
   is not always the case. Looking at security policy, we argue that when
   policy responsibility is concentrated in a single actor the effect of
   institutions on blame attribution is different from what the economic
   voting literature predicts. Divided government in this context makes
   blame-shifting less effective and makes it more likely that citizens
   will punish incumbents. By contrast, the ability of executives to
   control the narrative around security failures by blaming the
   perpetrators, especially during unified government, can help them avoid
   blame.},
