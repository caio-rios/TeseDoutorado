@article{ WOS:000354262600004,
Author = {Roberts, Tyson L.},
Title = {The Durability of Presidential and Parliament-Based Dictatorships},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2015},
Volume = {48},
Number = {7},
Pages = {915-948},
Month = {JUN},
Abstract = {Many scholars have examined the durability of parliamentary versus
   presidential regimes in democracies, but the process by which
   authoritarian leaders are (nominally) elected is generally assumed to be
   irrelevant. However, even if the electoral outcome is not in doubt, the
   executive selection process may affect outcomes such as regime
   durability. I argue that, when opposition parties are allowed to
   participate in the executive selection process, a Parliament-based
   election system increases authoritarian regime durability by creating
   incentives for ruling party elites to work cooperatively for mutual
   gain. In this article, I identify dictatorships by executive selection
   system and find that regimes with a multiparty Parliament-based system
   are more durable than those with a multiparty Presidential system or an
   Unelected (or single party) system if the party, relative to the ruler
   and the military, is an important power center.},
