@article{ WOS:000357958700004,
Author = {Hobolt, Sara B. and de Vries, Catherine E.},
Title = {Issue Entrepreneurship and Multiparty Competition},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2015},
Volume = {48},
