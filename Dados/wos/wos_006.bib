@article{ WOS:000341183400003,
Author = {Ezrow, Lawrence and Tavits, Margit and Homola, Jonathan},
Title = {Voter Polarization, Strength of Partisanship, and Support for Extremist
   Parties},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2014},
Volume = {47},
Number = {11},
Pages = {1558-1583},
Month = {SEP},
Abstract = {In this study, we evaluate how voter polarization and the level of
   partisanship influence electoral outcomes. We show that when the level
   of partisanship is low, the polarization of voter preferences translates
   into popular support for extreme parties. In contrast, longstanding
   attachments to mainstream (moderate) parties dampen the relationship
   between voter polarization and support for extreme parties. The
   implication of these findings is that the lack of voter attachment to
   parties contributes to extreme party competition, while strong
   attachment can help reduce party extremism even if electorates are
   polarized.},
