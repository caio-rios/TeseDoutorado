@article{ WOS:000357764600004,
Author = {Buckley, David T.},
Title = {Beyond the Secularism Trap Religion, Political Institutions, and
   Democratic Commitments},
Journal = {COMPARATIVE POLITICS},
Year = {2015},
Volume = {47},
Number = {4},
Pages = {439+},
Month = {JUL},
Abstract = {The often-tense relationship between religion and democracy can fuel a
   ``secularism trap,{''} in which divisions among religious and secular
   actors threaten the viability of democracy itself. What explains why
   such divisions threaten in some cases, but not others? I argue that
   political institutions, in particular institutional ties between
   religion and state, account for this variation. One institutional type,
   which I term ``benevolent secularism,{''} reduces the likelihood of the
   secularism trap even in least-likely cases. Institutions matter because
   they impact religious and secular elites in two distinct ways: shaping
   preferences within each bloc and promoting credible commitments to
   democracy between them. I trace the observable implications of this
   theory with evidence from two states that seemed likely candidates for
   the secularism trap: Ireland and Senegal.},
