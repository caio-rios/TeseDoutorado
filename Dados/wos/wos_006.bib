@article{ WOS:000353005800005,
Author = {Themner, Anders},
Title = {Former Military Networks and the Micro-Politics of Violence and
   Statebuilding in Liberia},
Journal = {COMPARATIVE POLITICS},
Year = {2015},
Volume = {47},
Number = {3},
Pages = {334+},
Month = {APR},
Abstract = {Recent studies have highlighted the inability of disarmament,
   demobilization, and reintegration programs to dismantle command
   structures in the aftermath of civil war. The effect that lingering
   military networks have on peace, however, is ambiguous. Therefore, a key
   question which has so far been unanswered is why some ex-military
   networks are remobilized for violent purposes while others are used for
   more productive ones, such as income-generating activities. In this
   article, I seek to address this question by comparing two former
   mid-level commanders (ex-MiLCs) in Liberia and the networks that they
   control. Based on this comparison I argue that it is ex-MiLCs who are
   shunned by governing elites as peacetime brokers of patronage
   distributing economic resources to ex-fighters that are most likely to
   remobilize their ex-combatant networks.},
Publisher = {SHERIDAN PRESS},
Address = {COMPARATIVE POLITICS, PO BOX 465, HANOVER, PA 17331 USA},
Type = {Article},
Language = {English},
DOI = {10.5129/001041515814709284},
ISSN = {0010-4159},
