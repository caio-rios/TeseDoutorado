@article{ WOS:000369998700009,
Author = {Reutter, Werner and Lorenz, Astrid},
Title = {Explaining the Frequency of Constitutional Change in the German
   Lander: Institutional and Party Factors},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2016},
Volume = {46},
Number = {1},
Pages = {103-127},
Month = {WIN},
Abstract = {In this article, we try to explain why some German Land constitutions
   are changed more frequently than others. Notably, we consider three
   arguments: first, we point out that subnational constitutional politics
   are an important, permanent, and salient issue in the German Lander.
   Second, we examine the extent to which parties and features of
   subnational constitutions impact the number of constitutional amendments
   in the German Lander. The analysis shows that both institutional and
   party factors partially explain the policy variation. Third, based on
   these findings, we conclude that theories trying to explain subnational
   constitutional amendments need to combine institutional
   andactor-centered approaches. Furthermore, German consensus democracy
   seems to work in a specific way at the Lander level.},
