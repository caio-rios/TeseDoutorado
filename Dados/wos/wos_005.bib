@article{ WOS:000398243700005,
Author = {Bernhard, Michael and Jung, Dong-Joon},
Title = {Civil Society and Income Inequality in Post-Communist Eurasia},
Journal = {COMPARATIVE POLITICS},
Year = {2017},
Volume = {49},
Number = {3},
Pages = {373+},
Month = {APR},
Abstract = {This article argues that the strength of civil society at the point of
   extrication from communism is a powerful predictor of how ``liberal
   democratic{''} post-communist regimes become. This is based on the
   impact that an engaged civil society has on the reconfiguration of
   post-communist elites and the degree to which the model of accumulation
   permits concentration of resources in the hands of previous elites. In
   cases where civil society was engaged at the moment of extrication, the
   elite were disposed to a more liberal model of capitalism which afforded
   greater social welfare protection. Where civil society was weaker, the
   elite were able to convert political power into concentrated control of
   economic assets and a more predatory and inegalitarian model of
   political capitalism emerged.},
Publisher = {SHERIDAN PRESS},
