@article{ WOS:000388310500004,
Author = {Anria, Santiago and Niedzwiecki, Sara},
Title = {Social Movements and Social Policy: the Bolivian Renta Dignidad},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
