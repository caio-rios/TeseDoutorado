@article{ WOS:000412287000005,
Author = {Drew, Joseph and Grant, Bligh},
Title = {Subsidiarity: More than a Principle of Decentralization-a View from
   Local Government},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2017},
Volume = {47},
Number = {4},
Pages = {522-545},
Month = {FAL},
Abstract = {A common interpretation of the principle of subsidiarity in the
   federalism literature is that decentralized government, which is closer
   to the people, is better able to respond to the preferences of its
   citizens. However, when the principle is denuded of its moral
   foundations in this fashion it not only fails to provide the grounding
   for achieving human dignity and the common good, but may also become the
   harbinger of fiscal crises and social dysfunction. We provide a more
   comprehensive account of the principle of subsidiarity and contrast this
   with various conceptions prominently presented in the federalism
   literature. We then explore how this more comprehensive view of
   subsidiarity would look in practice. In short, we argue that mere
   decentralization of government fails to capture the ontology and
   desirable outcomes of the principle of subsidiarity.},
