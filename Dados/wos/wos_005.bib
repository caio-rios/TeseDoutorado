@article{ WOS:000401712400003,
Author = {Steeves, H. Leslie and Kwami, Janet},
Title = {Interrogating Gender Divides in Technology for Education and
   Development: the Case of the One Laptop per Child Project in Ghana},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
