@article{ WOS:000425359600004,
Author = {Ricart-Huguet, Joan and Green, Elliott},
Title = {Taking it Personally: the Effect of Ethnic Attachment on Preferences for
   Regionalism},
Journal = {STUDIES IN COMPARATIVE INTERNATIONAL DEVELOPMENT},
