@article{ WOS:000390592500005,
Author = {Herrmann, Julian Durazo},
Title = {Media and subnational democracy: the case of Bahia, Brazil},
Journal = {DEMOCRATIZATION},
Year = {2017},
Volume = {24},
Number = {1},
Pages = {81-99},
Month = {JAN},
Abstract = {It is a well-known fact that the transition to and consolidation of
   democracy in Latin America have been problematic, especially at the
   subnational level. It is also commonplace to equate an independent media
   system with a strong democracy. While each of these fields has witnessed
   important developments in the last decade or so, there have been sparse
   attempts to draw the theoretical links between them. In this article, I
   argue that there are important insights to be gained from such an
   endeavour. Bahia, a state in north-eastern Brazil, is an ideal case
   study to bring these perspectives together. This study offers fresh
   insights on state-society relations at the subnational level and on the
   contemporary interaction between the public and the private spheres in
   Latin America. Last but not least, it will also provide a better grasp
   on the challenges democratization faces at the subnational level and the
   role of the media in them.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
