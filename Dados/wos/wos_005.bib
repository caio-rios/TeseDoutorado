@article{ WOS:000429396000002,
Author = {McFaul, Michael},
Title = {Choosing Autocracy: Actors, Institutions, and Revolution in the Erosion
   of Russian Democracy},
Journal = {COMPARATIVE POLITICS},
Year = {2018},
Volume = {50},
Number = {3},
Pages = {305+},
Month = {APR},
Abstract = {Russia's present system of government did not result inevitably from
   historical structures, that is from cultural, geographic, or
   socio-economic inheritances from the Soviet or tsarist past. Russia's
   hundreds of years of autocratic traditions made democratic consolidation
   in the 1990s harder, but not impossible. Rather, individual choices at
   pivotal moments in time pushed Russia towards a more autocratic path in
   the 2000s and then produced a reordering of preferences and power in
   favor of continuity with this new autocratic arrangement. Actors, not
   structures, were the drivers of these changes, first towards democracy
   and then away from it.},
