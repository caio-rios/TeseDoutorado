DOI = {10.1177/0010414015626446},
ISSN = {0010-4140},
EISSN = {1552-3829},
Keywords = {elections; public opinion; voting behavior; quality of democracy;
   democratic theory; political parties; presidents and executive politics},
Keywords-Plus = {ECONOMIC-CONDITIONS; MODEL; PARTIES; PREFERENCES; DYNAMICS; PATTERNS;
