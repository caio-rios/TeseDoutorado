@article{ WOS:000390976100006,
Author = {Rhodes-Purdy, Matthew},
Title = {Beyond the Balance Sheet Performance, Participation, and Regime Support
   in Latin America},
Journal = {COMPARATIVE POLITICS},
Year = {2017},
Volume = {49},
Number = {2},
Pages = {252+},
Month = {JAN},
Abstract = {This article examines incongruities between policy performance and
   regime support in Chile and Venezuela. Democratic theory and political
   psychology suggest that intrinsic characteristics of regime procedures,
   especially the extent to which those procedures provide citizens with a
   meaningful political role, can influence support independently of policy
   outcomes. I find that Chile's elitist democracy has created an enervated
   populace, leading to anemic support. Conversely, Venezuela's provisions
   for direct participatory opportunities help to legitimate the Bolivarian
   regime, in spite of its authoritarian tendencies, by encouraging a sense
   of control and efficacy among its citizens.},
