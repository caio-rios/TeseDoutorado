@article{ WOS:000396524700002,
Author = {Guimaraes, Carolina and Piefer, Nadine},
Title = {Brazil: (Future) green energy power and strategic partner for the EU?},
Journal = {COMPARATIVE EUROPEAN POLITICS},
Year = {2017},
