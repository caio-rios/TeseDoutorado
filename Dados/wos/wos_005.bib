@article{ WOS:000384396100005,
Author = {Miller, Michael K.},
Title = {Democracy by Example? Why Democracy Spreads When the World's Democracies
   Prosper},
Journal = {COMPARATIVE POLITICS},
Year = {2016},
Volume = {49},
Number = {1},
Pages = {83+},
Month = {OCT},
Abstract = {Does a positive association between democracy and economic growth around
   the world encourage the spread of democracy? Although this intuitive
   relationship has been linked to the global ebb and flow of fascism and
   Communism, no study has empirically tested this question. I argue that
   democracy's relative economic success in the world influences
   perceptions of its domestic advantages and thereby shifts popular and
   elite preferences in favor of democracy. Looking at 172 countries from
   18202010, I show that the world-level correlation between democracy and
   economic growth robustly predicts the spread of democracy and represents
   a major source of its historical advance. The results provide new
   insights into the foreign influences on democratization, China's
   challenge to the liberal democratic order, and the political legacy of
   the global financial crisis.},
