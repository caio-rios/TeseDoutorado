@article{ WOS:000429781400001,
Author = {Rozenas, Arturas and Sadanandan, Anoop},
Title = {Literacy, Information, and Party System Fragmentation in India},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2018},
Volume = {51},
Number = {5},
Pages = {555-586},
Month = {APR},
Abstract = {6 A rich theoretical literature argues that, in contradiction to
   Duverger's law, the plurality voting rule can fail to produce two-party
   system when voters do not share their common information about the
   electoral situation. We present an empirical operationalization and a
   series of tests of this informational hypothesis in the case of India
   using constituency- and individual-level data. In highly illiterate
   constituencies where access to information and information sharing among
   voters is low, voters often fail to coordinate on the two most viable
   parties. In highly literate constituencies, voters are far more
   successful at avoiding vote-wastingin line with the informational
   hypothesis. At a microlevel, these aggregate-level patterns are driven
   by the interaction of individual information and the informational
   context: In dense informational environments, even low-information
   voters can successfully identify viable parties and vote for them, but
   in sparse informational environments, individual access to information
   is essential for successful strategic voting.},
