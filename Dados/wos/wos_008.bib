@article{ WOS:000282333600003,
Author = {Spektorowski, Alberto and Ireni-Saban, Liza},
Title = {Staying Alive: Genetics in the Service of the Welfare ``People's
   Home{''}},
Journal = {COMPARATIVE POLITICAL STUDIES},
