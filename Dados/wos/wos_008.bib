@article{ WOS:000289199200006,
Author = {Okpanachi, Eyene},
Title = {Federalism and Economic Growth: The Importance of Context in Nigerian
   Public Finance Reform},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2011},
Volume = {41},
Number = {2},
Pages = {311-335},
Month = {SPR},
Abstract = {This article examines public finance reforms in the Nigerian federal
   system against the background of its transition from military to
   democratic rule in 1999 and the challenges posed to overall outcomes,
   especially by the fiscal health of the sub-national units. Using a
   before-and-after assessment of policies and performances, the article
   highlights the progress made in reforming public finance, and discusses
   the formidable political, institutional and social constraints that have
   made reforms difficult and inefficient. By focusing on the Nigerian case
   information, the article contributes to the debates on whether or not,
   and when federalism can contribute to economic growth and prosperity.},
