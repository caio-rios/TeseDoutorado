@article{ WOS:000282624400001,
Author = {Mazzuca, Sebastian},
Title = {Macrofoundations of Regime Change Democracy, State Formation, and
   Capitalist Development},
Journal = {COMPARATIVE POLITICS},
Year = {2010},
Volume = {43},
Number = {1},
Pages = {1+},
Month = {OCT},
Abstract = {The tradition of historical political sociology has produced highly
   sophisticated explanations of variations in national political regimes
   However, a series of false debates has interfered with further progress
   Whereas the various hypotheses in conflict within historical political
   sociology are substantially more similar than it seems, the outcomes
   explained are actually different. Even the most antagonistic
   perspectives-no-Marxism and neo-Weberianism-have two theoretical
   underpinnings in common-a conceptual framework centered on the notion of
   power and an explanatory logic that views national political regimes as
   local adaptations to a universal transformation in the organization of
   power Rather than opposite explanations of the same aggregate outcome,
   they should be understood as mutually independent hypotheses about the
   different components of political regimes},
