@article{ WOS:000279483300006,
Author = {Burke, Brendan and Ferguson, Margaret},
Title = {Going Alone or Moving Together: Canadian and American Middle Tier
   Strategies on Climate Change},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
