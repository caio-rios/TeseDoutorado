@article{ WOS:000302483800004,
Author = {Zehavi, Amos J.},
Title = {Veto Players, Path Dependency, and Reform of Public Aid Policy toward
   Private Schools Australia, New Zealand, and the United States},
Journal = {COMPARATIVE POLITICS},
Year = {2012},
Volume = {44},
Number = {3},
Pages = {311+},
Month = {APR},
Abstract = {Two main weaknesses of the new institutionalism literature, one
   associated with veto player theories and the other with path dependency
   theories, help account for the framework's inadequacy in theorizing
   change, especially radical change. A comparison of the divergent
   development of public aid to private schools in three
   countries-Australia, New Zealand, and the United States-demonstrates the
   importance of two factors that potentially mitigate the status
   quo-preserving effect of veto points and path dependency: the
   multifaceted nature of veto points and the political clout of status quo
   challengers. Even in what initially appear to be highly constrained
   institutional systems, significant reform can occur, at either a rapid
   or slow pace.},
