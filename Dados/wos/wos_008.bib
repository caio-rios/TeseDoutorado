   Weghorst, Keith R.; Lindberg, Staffan I., Univ Florida, Dept Polit Sci, Gainesville, FL 32611 USA.
   Lindberg, Staffan I., Univ Florida, Ctr African Studies, Gainesville, FL 32611 USA.},
DOI = {10.1080/13510347.2011.603483},
ISSN = {1351-0347},
EISSN = {1743-890X},
