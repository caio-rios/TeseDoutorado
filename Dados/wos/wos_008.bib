@article{ WOS:000306373700008,
Author = {Shortell, Christopher},
Title = {The End of the Federalism Five? Statutory Interpretation and the Roberts
   Court},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2012},
Volume = {42},
Number = {3, SI},
Pages = {516-537},
Month = {SUM},
Abstract = {The replacement of Chief Justice Rehnquist and Justice O'Connor with
   John Roberts and Samuel Alito led to much uncertainty about the future
   of federalism jurisprudence. Six terms in to the Roberts Court, clear
   patterns of difference from the Rehnquist Court are emerging. My
   analysis of all federalism decisions by the high court since John
   Roberts was sworn in as chief justice in 2005 demonstrates that the
   Federalism Five bloc of justices is no longer the dominant paradigm for
   understanding responses to federalism cases. The emphasis on preemption
   cases and the increased role of statutory rather than constitutional
   interpretation have led to shifting coalitions and a different course
   for federalism cases on the Roberts Court, although there are legitimate
   questions about whether this will continue.},
