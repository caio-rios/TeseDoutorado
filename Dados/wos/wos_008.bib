@article{ WOS:000282167500004,
Author = {Mendez, Fernando and Mendez, Mario},
Title = {Comparing Privacy Regimes: Federal Theory and the Politics of Privacy
   Regulation in the European Union and the United States},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2010},
Volume = {40},
Number = {4},
Pages = {617-645},
Month = {FAL},
Abstract = {This article draws on federal theory to analyze the dynamics of crafting
   a privacy regime in the European Union and the United States. Three
   dimensions of the policy domain are analyzed: how policy was framed, the
   policy dynamics activated, and the policy instruments used. The main
   finding is that despite obvious differences in the choice of specific
   policy instruments, a strikingly similar regulatory dynamic and framing
   of policy can be detected. Two conclusions are drawn: first, federal
   theory can reveal novel insights on the politics of regulating privacy,
   especially in relation to new technologies and counter terrorism
   measures. Second, applying a comparative research design to EU
   policy-making can help explain many of its apparent empirical puzzles.},
