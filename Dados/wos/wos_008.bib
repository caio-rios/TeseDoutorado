@article{ WOS:000292782800005,
Author = {Balan, Manuel},
Title = {Competition by Denunciation The Political Dynamics of Corruption
   Scandals in Argentina and Chile},
Journal = {COMPARATIVE POLITICS},
Year = {2011},
Volume = {43},
Number = {4},
Pages = {459+},
Month = {JUL},
Abstract = {Corruption has become a key concern throughout the world. Most of what
   is known about corruption comes from instances in which misdeeds become
   public, generating a scandal. Why do some acts of corruption become
   scandals and others do not? Corruption scandals are not triggered by
   corruption, but rather are initially caused by dynamics of political
   competition within government. Insiders leak information on misdeeds in
   order to gain power within the coalition or party in power. A powerful
   opposition, contrary to common belief, acts as a constraint for
   insiders, making corruption scandals less likely. These arguments are
   evaluated using empirical evidence from Argentina and Chile (1989-2008).
   The findings support the notion that corruption scandals emerge as a
   consequence of political competition.},
