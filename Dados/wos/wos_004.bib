@article{ WOS:000504503000001,
Author = {Brambor, Thomas and Goenaga, Agustin and Lindvall, Johannes and Teorell,
   Jan},
Title = {The Lay of the Land: Information Capacity and the Modern State},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2020},
Volume = {53},
Number = {2},
Pages = {175-213},
Month = {FEB},
Abstract = {This article presents new evidence on the efforts of states to collect
   and process information about themselves, their territories, and their
   populations. We compile data on five institutions and policies: the
   regular implementation of a reliable census, the regular release of
   statistical yearbooks, the introduction of civil and population
   registers, and the establishment of a government agency tasked with
   processing statistical information. Using item response theory methods,
   we generate an index of ``information capacity{''} for 85 states from
   1789 to the present. We then ask how political regime changes have
   influenced the development of information capacity over time. In
   contrast with the literature on democracy and fiscal capacity, we find
   that suffrage expansions are associated with higher information
   capacity, but increases in the level of political competition are not.
   These findings demonstrate the value of our new measure, because they
   suggest that different elements of state capacity are shaped by
   different historical processes.},
