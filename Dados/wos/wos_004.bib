@article{ WOS:000488869100006,
Author = {Garay, Candelaria and Marta Maroto, Maria},
Title = {Local Health Care Provision as a Territorial Power-Building Strategy:
   Non-Aligned Mayors in Argentina},
Journal = {COMPARATIVE POLITICS},
Year = {2019},
Volume = {52},
Number = {1},
Pages = {105+},
Month = {OCT},
Abstract = {What explains variation in local health services? Comparative
   scholarship highlights economic factors, electoral competition, and
   partisanship to account for service disparities. Employing an original
   data set and qualitative case studies of health service provision in
   thirty-three metropolitan municipalities in Argentina between 1995 and
   2015, we find that mayors are likely to provide more services when they
   are not aligned with the governor. Unable to access the discretionary
   resources and electoral support from governors that aligned mayors
   enjoy, non-aligned mayors exploit automatic provincial revenue-sharing
   health transfers, which reward municipalities that provide more services
   and have more infrastructure, in order to build territorial power. These
   findings highlight the importance of non-alignment and the conditions
   under which formula-based transfers encourage local service provision.},
