@article{ WOS:000457493700004,
Author = {Gruffydd-Jones, Jamie J.},
Title = {Citizens and Condemnation: Strategic Uses of International Human Rights
   Pressure in Authoritarian States},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2019},
Volume = {52},
Number = {4},
Pages = {579-612},
Month = {MAR},
Abstract = {Governments with strict control over the information that their citizens
   hear from foreign sources are regular targets of human rights pressure,
   but we know little about how this information matters in the domestic
   realm. I argue that authoritarian regimes strategically pass on certain
   types of external pressure to their public to ``internationalize{''}
   human rights violations, making citizens view human rights in terms of
   defending their nation internationally rather than in terms of
   individual violations, and making them more likely to be satisfied with
   their government's behavior. I find strong support for this model
   through statistical analysis of Chinese state media reports of external
   human rights pressure and a survey experiment on Chinese citizens'
   responses to pressure on women's rights. This analysis demonstrates that
   authoritarian regimes may be able to manipulate international human
   rights diplomacy to help them retain the support of their population
   while suppressing their human rights.},
