@article{ WOS:000474997000001,
Author = {Clayton, Amanda and Robinson, Amanda Lea and Johnson, Martha C. and
   Muriaas, Ragnhild},
Title = {(How) Do Voters Discriminate Against Women Candidates? Experimental and
   Qualitative Evidence From Malawi},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2020},
Volume = {53},
Number = {3-4},
Pages = {601-630},
Month = {MAR},
Abstract = {How do voters evaluate women candidates in places where traditional
   gender norms are strong? We conduct a survey experiment in Malawi to
   assess both whether citizens discriminate against women candidates and
   how other salient candidate characteristics-political experience, family
   status, policy focus, and gendered kinship practices-interact with
   candidate gender to affect citizen support. Contrary to our
   expectations, we find citizens prefer women candidates ceteris paribus,
   and women and men with the same traits are evaluated similarly. Yet, we
   find two unexpected ways women candidates are disadvantaged in the
   electoral process. First, we find that citizens prefer candidates who
   are married with young children, a profile much more common among men
