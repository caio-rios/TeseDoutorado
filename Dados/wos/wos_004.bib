@article{ WOS:000537477800008,
Author = {Flavin, Patrick and Shufeldt, Gregory},
Title = {Explaining State Preemption of Local Laws: Political, Institutional, and
   Demographic Factors},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2020},
Volume = {50},
Number = {2},
Pages = {280-309},
Month = {SPR},
Abstract = {Despite increasing popular and media attention to the preemption of
   local policymaking by state governments, the empirical political science
   literature on preemption remains relatively scarce. After first
   identifying and discussing state preemption laws across twenty-one
   diverse public policies, we investigate how political, institutional,
   and demographic factors predict the implementation of these laws. Our
   empirical analysis reveals that states where Republicans control both
   legislative chambers and the governorship, with more politically
