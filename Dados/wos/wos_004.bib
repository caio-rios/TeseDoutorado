@article{ WOS:000445268600002,
Author = {Cedermanl, Lars-Erik and Gleditsch, Kristian Skrede and Wucherpfennig,
   Julian},
Title = {The Diffusion of Inclusion: An Open-Polity Model of Ethnic Power Sharing},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2018},
Volume = {51},
Number = {10},
Pages = {1279-1313},
Month = {SEP},
Abstract = {While there is a growing consensus that ethnic inclusion produces peace,
   less is known about what causes transitions to power sharing between
   ethnic groups in central governments in multiethnic states. The few
   studies that have addressed this question have proposed explanations
   stressing exclusively domestic factors. Yet, power sharing is spatially
   clustered, which suggests that diffusion may be at play. Inspired by
   studies of democratic diffusion, we study the spread of inclusive
   policies with an ``open-polity model{''} that explicitly traces
   diffusion from inclusion in other states. Our findings indicate that the
   relevant diffusion processes operate primarily at the level of world
   regions rather than globally or between territorial neighbors. Thus, the
   more inclusive the region, the more likely a shift to power sharing
   becomes. Shifts away from inclusion to dominance are less common since
   World War II, but they are more likely in regional settings
   characterized by ethnic exclusion.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Cedermanl, LE (Corresponding Author), Swiss Fed Inst Technol, Haldeneggsteig 4, CH-8092 Zurich, Switzerland.
   Cedermanl, Lars-Erik, Swiss Fed Inst Technol, Int Conflict Res, Zurich, Switzerland.
   Gleditsch, Kristian Skrede, Univ Essex, Dept Govt, Colchester, Essex, England.
   Gleditsch, Kristian Skrede, Peace Res Inst Oslo, Oslo, Norway.
   Wucherpfennig, Julian, Hertie Sch Governance, Berlin, Germany.},
DOI = {10.1177/0010414017740602},
ISSN = {0010-4140},
