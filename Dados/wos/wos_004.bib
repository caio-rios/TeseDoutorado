@article{ WOS:000443286200001,
Author = {Carlson, Elizabeth},
Title = {The Relevance of Relative Distribution: Favoritism, Information, and
   Vote Choice in Africa},
Journal = {COMPARATIVE POLITICAL STUDIES},
