@article{ WOS:000469848400004,
Author = {Aliyev, Huseyn},
Title = {The Logic of Ethnic Responsibility and Progovernment Mobilization in
   East Ukraine Conflict},
Journal = {COMPARATIVE POLITICAL STUDIES},
