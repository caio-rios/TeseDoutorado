@article{ WOS:000456647600005,
Author = {Barrio, Astrid and Barbera, Oscar and Rodriguez-Teruel, Juan},
Title = {`Spain steals from us!' The `populist drift' of Catalan regionalism},
Journal = {COMPARATIVE EUROPEAN POLITICS},
