@article{ WOS:000627104000001,
Author = {Bornschier, Simon and Hausermann, Silja and Zollinger, Delia and
   Colombo, Celine},
Title = {How ``Us{''} and ``Them{''} Relates to Voting Behavior-Social Structure,
   Social Identities, and Electoral Choice},
