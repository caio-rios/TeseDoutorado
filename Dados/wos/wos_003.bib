@article{ WOS:000573216500004,
Author = {Hatab, Shimaa},
Title = {Threat Perception and Democratic Support in Post-Arab Spring Egypt},
Journal = {COMPARATIVE POLITICS},
Year = {2020},
Volume = {53},
Number = {1},
Pages = {69+},
Month = {OCT},
Abstract = {The article examines the reasons why Egyptian elites and masses withdrew
   their support for democracy only two years after they staged mass
   protests calling for regime change in 2011. I draw on basic tenets of
   bounded rationality and recent advances within the field of cognitive
   heuristics to demonstrate how cues generated from domestic and regional
   developments triggered stronger demands for security and stability.
   Drawing on elite interviews and public opinion surveys, I show how both
   elites and the masses paid special attention to intense and vivid events
   which then prompted a demand for the strong man model. Fears of
   Islamists pushed both elites and masses to update their preferences,
   seek refuge in old regime bargains, and reinstate authoritarianism.},
Publisher = {SHERIDAN PRESS},
Address = {COMPARATIVE POLITICS, PO BOX 465, HANOVER, PA 17331 USA},
