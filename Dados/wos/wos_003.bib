@article{ WOS:000685218000010,
Author = {Sinclair, J. Andrew and Love, Maya and Gutierrez-Vera, Maria},
Title = {Federalism, Defunding the Police, and Democratic Values: A Functional
   Accountability Framework for Analyzing Police Reform Proposals},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2021},
Volume = {51},
Number = {3},
Pages = {484-511},
Month = {SUM},
Abstract = {This article examines recent proposals to ``defund the police{''} using
   a perspective informed by literature on agency termination and political
   accountability. These concepts allow us to build a framework for
   categorizing such proposals based on the assignment of functions to
   organizations. These proposals operate in the context of a federal
   system which can shape the character of each category, make some choices
   more or less feasible, influence where functions may be assigned, and
   create political incentives out of the complex geography of conflict. We
   use examples from Minneapolis, NewYork, Los Angeles, and Seattle to
   illustrate our framework. We conclude by offering a preliminary analysis
   of why these cities tend towards adopting one of the alternatives we
   outline.},
