@article{ WOS:000593161400005,
Author = {Jacobs, Nicholas F. and Munis, B. K.},
Title = {Staying in Place: Federalism and the Political Economy of Place
   Attachment},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2020},
Volume = {50},
Number = {4},
Pages = {544-565},
Month = {FAL},
Abstract = {A growing number of scholars have documented how social identities
   defined by an attachment to place influence individuals' understandings
   about political power and representation. Drawing on this theoretical
   framework, we explore how place-based identities matter for American
   federalism by documenting how attachments to the American states alter
   individuals' decisions to leave, or exit, as well as to welcome
   newcomers into their local communities. Using a set of conjoint
   experiments designed to measure individual attitudes about place,
   politics, and America's federal polity, we find evidence that Americans
   hold deep and consequential attitudes about the places in which they
   live. Our evidence confirms that state identities are still highly
   relevant in shaping American federalism and the competitive pressures
   between intergovernmental jurisdictions. While federalism may encourage
   individuals to leave, federalism also nourishes place-specific
   attachments, motivating people to stay.},
