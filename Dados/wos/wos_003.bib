@article{ WOS:000646838000001,
Author = {Mochtak, Michal and Lesschaeve, Christophe and Glaurdic, Josip},
Title = {Voting and winning: perceptions of electoral integrity in consolidating
   democracies},
Journal = {DEMOCRATIZATION},
Year = {2021},
Volume = {28},
