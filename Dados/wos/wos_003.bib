@article{ WOS:000550219500002,
Author = {Tellez, Juan Fernando and Wibbels, Erik and Krishna, Anirudh},
Title = {Local Order, Policing, and Bribes Evidence from India},
Journal = {WORLD POLITICS},
Year = {2020},
