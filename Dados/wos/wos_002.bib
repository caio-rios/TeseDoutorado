@article{ WOS:000805528900001,
Author = {Lim, Junghyun},
Title = {The Electoral Consequences of International Migration in Sending
   Countries: Evidence from Central and Eastern Europe},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2023},
Volume = {56},
Number = {1},
Pages = {36-64},
Month = {JAN},
Abstract = {This paper examines the political attributes of emigrants and how their
   departure affects the electoral outcomes in their home countries. I
   argue that emigrants are different from those who remain in their
   political preferences as well as economic attributes, such that
   large-scale emigration changes the distribution of voters in sending
   countries. Emigration can also directly affect the policy preferences of
   individuals who stay in their home countries. I test these arguments in
   seven Central and Eastern European countries, using individual-level
   surveys and region-level data on emigration and elections. To address
   potential endogeneity issues, I use instrumental variable analysis,
   leveraging the surge of Polish emigration to the United Kingdom after
   the EU enlargement. I find that emigrants from Central and Eastern
   Europe tend to be younger, highly educated, and politically more
   progressive and that the vote shares of far-right parties are larger in
   regions with higher emigration rates. Also, I find that exposure to
   large-scale emigration affects the vote choices of individuals who
   remain.},
