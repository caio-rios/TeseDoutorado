@article{ WOS:000842574900001,
Author = {Stahl, Rune Moller and Rosamond, Ben},
Title = {Non-majoritarian institutions: two strands of liberalism in European
   economic governance},
Journal = {COMPARATIVE EUROPEAN POLITICS},
Year = {2022},
Volume = {20},
