@article{ WOS:000978598300001,
Author = {Corrochano, David Hernandez and Lopez, Teresa Mata and Rodriguez,
   Leticia Ruiz M.},
Title = {Radicality and moderation in the language of Europe's extreme right},
Journal = {COMPARATIVE EUROPEAN POLITICS},
Year = {2024},
Volume = {22},
Number = {2},
Pages = {243-266},
Month = {APR},
Abstract = {Following the resurgence of the extreme right in Europe, we have
   witnessed several attempts to classify these parties and group them
   together under a common label. These efforts have shown that there are
   significant differences within this ``family{''}. Some of these
   differences, such as the variations in the degree of radicalisation of
   their discourses, have not received the attention they merit. The goal
   of this research is to show that these differences in radicality
   indicate how individual parties adapt, through the use of language, in
   response to the specific cultural contexts in which they operate. To
   this end, we have performed an analysis of the language employed in five
   party platforms, linking the differences to the determinants of their
   environments.},
