@article{ WOS:000818543800001,
Author = {Edwards, Pearce},
Title = {Political Competition and Authoritarian Repression Evidence from
   Pinochet's Chile},
Journal = {WORLD POLITICS},
Year = {2022},
Volume = {74},
Number = {4},
Pages = {479-522},
Month = {OCT},
Abstract = {Authoritarian regimes repress to prevent mass resistance to their rule.
   In doing so, regimes' security forces require information about the
   dissidents who mobilize such resistance. Political competition, which
   fuels partisan rivalries, offers one solution to this problem by
   motivating civilians to provide needed information to security forces.
   Yet civilians share information about any political opponents, not just
   dissidents, which creates a challenge for regimes that want to target
   dissidents. Drawing on novel archival data from the immediate aftermath
   of the 1973 coup that brought Augusto Pinochet to power in Chile, a
   period that included civilian collaboration with repression, this
   article presents evidence that close pre-coup political competition is
   associated with more frequent repression and more targeting of
   non-dissidents. The author uses pre-coup democratic elections to measure
   political competition and addresses the challenge of estimating
   political preferences unaffected by repression. Qualitative evidence and
   further quantitative tests probe implications of the partisan rivalry
   mechanism and account for alternative explanations.},
