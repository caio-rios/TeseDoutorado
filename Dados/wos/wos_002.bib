@article{ WOS:000885221600001,
Author = {Ray, Pradeep Kanta and Klarin, Anton and Ray, Sangeeta},
Title = {The Impact of Institutional Formation on Firms' Strategic Choices in
   Knowledge Development, Absorptive Capacity and Vertical Integration},
