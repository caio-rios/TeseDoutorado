@article{ WOS:000824484400001,
Author = {Binev, Binio S.},
Title = {Post-Communist Junctures, the Left, and Illiberalism: Theory with
   Evidence from Central and Eastern Europe},
Journal = {COMPARATIVE POLITICAL STUDIES},
