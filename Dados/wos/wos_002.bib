@article{ WOS:000997573000001,
Author = {Bucci, Laura C.},
Title = {The Role of Federalism and the Centrality of State Governments for Labor
   Unions, Employment, and Organizing},
Journal = {PUBLIUS-THE JOURNAL OF FEDERALISM},
Year = {2023},
Volume = {53},
Number = {3, SI},
Pages = {462-483},
Month = {JUL 7},
Abstract = {American states differ in their protection of workers, acceptance of
   organized labor, dominant industrial cultures, and regarding labor
   unions' capacity and political power. In this article, I examine
   declining union membership in the states and analyze various laws that
   limit the political power of unions and assess the consequences of these
