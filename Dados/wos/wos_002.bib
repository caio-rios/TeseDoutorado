@article{ WOS:000737939400001,
Author = {Neundorf, Anja and Shorrocks, Rosalind},
Title = {Revisiting the Impact of Modernization on Support for Women Politicians:
   The Role of Women's Political Empowerment},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2022},
Volume = {55},
Number = {8},
Pages = {1298-1331},
Month = {JUL},
Abstract = {We examine the role of societal modernization and women's political
   empowerment in generating support for women politicians amongst
   citizens. Using a global analysis of 116 countries with a new dataset of
   micro- and macro-level longitudinal data, we show that societal
   modernization and women's political empowerment only have positive
   effects on support for women in politics when the other is also present.
   For citizens who experienced either societal modernization or women's
   political empowerment, but not both, we do not see this positive
   relationship. Crucially, these patterns hold when analysing the current
   social and political context, as well as the context experienced by
   citizens during their formative years. We argue that both social and
   political changes are required to develop supportive attitudes towards
   women in politics.},
