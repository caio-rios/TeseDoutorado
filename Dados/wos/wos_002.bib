@article{ WOS:000752458400001,
Author = {Somer-Topcu, Zeynep and Weitzel, Daniel},
Title = {Negative Campaigning and Vote Choice in Europe: How Do Different
   Partisan Groups React to Campaign Attacks?},
Journal = {COMPARATIVE POLITICAL STUDIES},
Year = {2022},
Volume = {55},
Number = {13},
Pages = {2283-2310},
Month = {NOV},
Abstract = {Parties spend parts of their campaigns criticizing other parties'
   performance and characteristics, such as honesty, integrity, and unity.
   These attacks aim to negatively affect the target parties' electoral
   performance. But do they work? While attacks are informative, we argue
   that how voters react to negative campaigning depends on their
   partisanship. While the target's copartisans are more likely to get
   mobilized in favor of their party, the attacker's copartisans are
   expected to punish the target due to their respective partisan
   motivations. We expect null effects for attacks for partisans of third
   parties as well as nonpartisans. Combining a new dataset on campaign
   rhetoric with survey data from eight European countries, we show support
   for most but not all of our expectations. These results have important
   implications for the electoral campaigns literature.},
